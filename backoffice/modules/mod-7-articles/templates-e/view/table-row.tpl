<tr>
    <td>{c2r-id}</td>
    <td>{c2r-title}</td>
    <td>{c2r-category-section}</td>
    <td>{c2r-parent-nr}</td>
    <td>{c2r-published}</td>
    <td title="{c2r-date-updated-label} : {c2r-date-updated}">{c2r-date-created}</td>
    <td class="taright">
        <a href="{c2r-path-bo}/{c2r-lg}/categories/view/{c2r-id}" class="btn btn-add{c2r-hide-but}" role="button">
			<i class="fa fa-eye" aria-hidden="true"></i><div class="block all-15"></div>{c2r-but-view}
		</a>
        <a href="{c2r-path-bo}/{c2r-lg}/categories/edit/{c2r-id}" class="btn btn-edit" role="button">
			<i class="fa fa-pencil" aria-hidden="true"></i><div class="block all-15"></div>{c2r-but-edit}
		</a>
        <a href="{c2r-path-bo}/{c2r-lg}/categories/delete/{c2r-id}" class="btn btn-cancel{c2r-show-but}" role="button">
			<i class="fa fa-pencil" aria-hidden="true"></i><div class="block all-15"></div>{c2r-but-delete}
		</a>
    </td>
</tr>
